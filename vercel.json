{
  "version": 2,
  "functions": {
    "portfolio_web/wsgi.py": {
      "runtime": "python3.9"
    }
  },
  "builds": [
    {
      "src": "main/static/**",
      "use": "@vercel/static"
    },
    {
      "src": "portfolio_web/wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/main/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/portfolio_web/wsgi.py"
    }
  ]
}